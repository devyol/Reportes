',
'
select t.descripcion
from(
select distinct descripcion, id
from(
select 
case 
	when pt."name" = ''Diesel'' then ''Diesel''
	else pt."name"
end as descripcion,
case 
	when pt."name" = ''Super'' then 1
	when pt."name" = ''Regular'' then 2
	when pt."name" = ''Diesel'' then 3
	
end as id
from product_template pt 
