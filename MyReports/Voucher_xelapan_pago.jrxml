	<queryString language="SQL">
		<![CDATA[select
rp."name" rp_name,
rp.street,
rp."ref" as nit,
ap.check_number,
ap.payment_date,
ap.amount,
prov."name" prov_name,
ap."name" ap_name,
li."name" "number",
ap.payment_date date_invoice,
ap.amount amount_total
from account_payment as ap
join mc_guatemala_liquidaciones as li
on li.id = ap.liquidaciones_id
join res_company as rc
on rc.id = ap.company_id
join res_partner as rp
on rp.id = rc.partner_id
join res_partner as prov
on prov.id = ap.partner_id
where
	case
		when ($P{p_cheque} is not null
		and $P{p_cheque} <> 0) then ap.check_number = $P{p_cheque}
		else ap."name" = $P{p_pago}
	end
order by
	ap.check_number]]>
	</queryString>
